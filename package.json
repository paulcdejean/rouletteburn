{
  "name": "rustburn",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "run-s build:*",
    "build:typescript": "tsc",
    "build:vite": "vite build",
    "build:obfuscate": "javascript-obfuscator dist/timeburn.js -o obfuscate/timeburn.js --config obfuscator.config.json",
    "build:bootstrap": "shx cat bootstrap.js obfuscate/timeburn.js > bitburner/timeburn.js"
  },
  "devDependencies": {
    "@types/node": "^20.3.1",
    "bitburner-filesync": "^1.1.5",
    "javascript-obfuscator": "^4.0.2",
    "npm-run-all": "^4.1.5",
    "shx": "^0.3.4",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vite-plugin-wasm": "^3.2.2"
  }
}
