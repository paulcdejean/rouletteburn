{
  "name": "timeburn",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "npm run build:rust && npm run build:lint && npm run build:typescript && npm run build:vite && npm run build:lint && npm run build:obfuscate",
    "build:rust": "wasm-pack build --target bundler --release",
    "build:lint": "eslint .",
    "build:typescript": "tsc",
    "build:vite": "vite build --mode production",
    "build:obfuscate": "javascript-obfuscator dist/timeburn.js -o bitburner/timeburn.js --config obfuscator.config.json"
  },
  "devDependencies": {
    "@types/react": "^17.0.52",
    "@types/react-dom": "^17.0.20",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "bitburner-filesync": "^1.1.5",
    "eslint": "^8.43.0",
    "javascript-obfuscator": "^4.0.2",
    "typescript": "~5.0.0",
    "vite": "^4.3.9",
    "vite-plugin-wasm": "^3.2.2"
  }
}
