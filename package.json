{
  "name": "timeburn",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "npm run build:lint && npm run build:typescript && npm run build:vite && npm run build:lint && npm run build:obfuscate",
    "build:lint": "eslint .",
    "build:typescript": "tsc",
    "build:vite": "vite build",
    "build:obfuscate": "javascript-obfuscator dist/timeburn.js -o obfuscate/timeburn.js --config obfuscator.config.json",
    "build:bootstrap": "shx cat capabilities/basic.js obfuscate/timeburn.js > bitburner/timeburn.js"
  },
  "devDependencies": {
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "bitburner-filesync": "^1.1.5",
    "eslint": "^8.43.0",
    "javascript-obfuscator": "^4.0.2",
    "shx": "^0.3.4",
    "typescript": "~5.0.0",
    "vite": "^4.3.9",
    "vite-plugin-wasm": "^3.2.2"
  }
}
